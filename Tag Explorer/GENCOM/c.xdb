		<FBProgram name="Com">
			<STSource>(* Type SEPAM
Atelier, type =1
FID, type =2
FIA, type =3
JDB, type = 4
Pompes, type = 5
Couplage, type =6
ONE, type =7
GTA, type =8
GES, type =9
TGBT, type =10*)

IF Index_SEPAM = Index_Com Then
SET (Com_active);
end_if;

if Com_active then
	IF not ERREUR_COM_MEMO  Then
	Status_Req[2]:=30;
		IF Etape=0 AND NOT Status_Req[0].0 THEN 
	(*lecture Zone Mesure*)
		READ_VAR(Adresse,'%MW',512,50,Status_Req,Req_MES_1_SEPAM);
			IF Status_Req[0].0 THEN 
			Etape:=1; 
			END_IF;
		END_IF;
		
		IF Etape=1 and NOT Status_Req[0].0 and Status_Req[1]=0 Then
		(*Pas Defaut com *)
		RESET (ERREUR_COM);
		Etape:=2;
		END_if;

		IF Etape=1 and NOT Status_Req[0].0 and Status_Req[1]&lt;&gt;0 Then
		(*Defaut com *)
		Nb_def_Test:= Nb_def_Test+1;
		Etape:=10;
		END_if;

		IF Etape=2 AND NOT Status_Req[0].0 THEN 
	(*lecture Zone Mesure*)
		READ_VAR(Adresse,'%MW',584,8,Status_Req,Req_MES_2_SEPAM);
			IF Status_Req[0].0 THEN 
			Etape:=3; 
			END_IF;
		END_IF;
		
		IF Etape=3 and NOT Status_Req[0].0 and Status_Req[1]=0 Then
		(*Pas Defaut com *)
		RESET (ERREUR_COM);
			if type_SEPAM= 5 then (* SEPAM Pompes *)
			Etape:=4;
			Else
			Etape:=6;
			End_if;
		END_if;

		IF Etape=3 and NOT Status_Req[0].0 and Status_Req[1]&lt;&gt;0 Then
		(*Defaut com *)
		Nb_def_Test:= Nb_def_Test+1;
		Etape:=10;
		END_if;

		IF Etape=4 AND NOT Status_Req[0].0 THEN 
	(*lecture Zone Mesure Pompes *)
		IF type_SEPAM= 5 THEN
			READ_VAR(Adresse,'%MW',768,30,Status_Req,Req_MES_Ppes_SEPAM);
			IF Status_Req[0].0 THEN Etape:=5; END_IF;
		ELSE Etape:=6; END_IF;
		END_IF;

		IF Etape=5 and NOT Status_Req[0].0 and Status_Req[1]=0 Then
		(*Pas Defaut com *)
		RESET (ERREUR_COM);
		Etape:=6;
		END_if;

		IF Etape=5 and NOT Status_Req[0].0 and Status_Req[1]&lt;&gt;0 Then
		(*Pas Defaut com *)
		Nb_def_Test:= Nb_def_Test+1;
		Etape:=10;
		END_if;

		IF Etape=6 AND NOT Status_Req[0].0 THEN 
	(*lecture Zone TOR *)
		READ_VAR(Adresse,'%MW',3088,19,Status_Req,Req_TOR_SEPAM);
		IF Status_Req[0].0 THEN Etape:=7; END_IF;
		END_IF;

		IF Etape=7 and NOT Status_Req[0].0 and Status_Req[1]=0 Then
		(*Pas Defaut com *)
		RESET (ERREUR_COM);
		Etape:=8;
		END_if;

		IF Etape=7 and NOT Status_Req[0].0 and Status_Req[1]&lt;&gt;0 Then
		(*Pas Defaut com *)
		Nb_def_Test:= Nb_def_Test+1;
		Etape:=10;
		END_if;
	
		IF Etape=8 AND NOT Status_Req[0].0 THEN 
	(*lecture Zone Protection SEPAM *)
		READ_VAR(Adresse,'%MW',3220,4,Status_Req,Req_PROTECT_SEPAM);
		IF Status_Req[0].0 THEN Etape:=9; END_IF;
		END_IF;

		IF Etape=9 and NOT Status_Req[0].0 and Status_Req[1]=0 Then
		(*Pas Defaut com *)
		RESET (ERREUR_COM);
		Etape:=10;
		END_if;

		IF Etape=9 and NOT Status_Req[0].0 and Status_Req[1]&lt;&gt;0 Then
		(*Pas Defaut com *)
		Nb_def_Test:= Nb_def_Test+1;
		Etape:=10;
		END_if;

		If Etape=10 then
		Index_Com := Index_Com +1;
		RESET (Com_active);
		Etape :=0;
		end_if;
	ELSE
	Index_Com := Index_Com +1; 
	END_IF;
END_IF;

If Nb_def_Test &gt;=5 THEN 
SET (ERREUR_COM);
SET (ERREUR_COM_MEMO);
END_if;


TOF_Def_com_memo (IN := ERREUR_COM_MEMO,
                  PT := T#120s);

If TOF_Def_com_memo.Q then
Nb_def_Test:=0;
RESET (ERREUR_COM_MEMO);
end_if;

(* Recopie Defaut Com *)
(*Defaut_com:= ERREUR_COM;*)
</STSource>
		</FBProgram>
		<FBProgram name="Recopie_MES">
			<STSource>(* Type SEPAM
