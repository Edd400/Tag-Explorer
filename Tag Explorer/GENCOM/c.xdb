(* Recopie Mesure SEPAM *)

SW_MES.I1 := DINT_TO_UDINT(INT_AS_DINT(Req_MES_1_SEPAM[512],Req_MES_1_SEPAM[513]));
SW_MES.I2 := DINT_TO_UDINT(INT_AS_DINT(Req_MES_1_SEPAM[514],Req_MES_1_SEPAM[515]));
