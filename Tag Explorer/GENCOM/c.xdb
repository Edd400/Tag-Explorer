ELSE
SW_MES.P := P_MAE;
END_IF;

If type_SEPAM= 4 then (* Type JDB *)
SW_MES.Frequence := Freq_MAE * 100.0;
ELSE
SW_MES.Frequence := DINT_TO_REAL(INT_AS_DINT(Req_MES_1_SEPAM[552],Req_MES_1_SEPAM[553]));
END_IF;

If (type_SEPAM= 4 or type_SEPAM= 7) then (* Type ONE *)
SW_MES.cosFI := cosFi_MAE * 100.0;
ELSE
SW_MES.cosFI := DINT_TO_REAL(INT_AS_DINT(Req_MES_1_SEPAM[560],Req_MES_1_SEPAM[561]));
END_if;

(* Recopie Mesure Pompes SEPAM *)
if type_SEPAM= 5 then
